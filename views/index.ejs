<body>
	<% include template/navbar.ejs %>

  <div class="contain">
    <div class="mask">
      <img class="w-50 mx-auto  d-block py-3" src="/photos/ask-me.png">
      <p class="text-center d-block">Join a session and feel free to ask</p>
      <button class="btn btn-lg mx-auto d-block btn-dark"><a href="/sessions">Find Your Session</a></button>
      <p class="text-center mt-4"><a href="/users/login">Log in</a> to vote question.</p>
      <p class="text-center">Not have an account? <a href="/users/login">Sign up</a></p>
    </div>
  </div>



  <!-- Optional JavaScript -->
  <!-- jQuery first, then Popper.js, then Bootstrap JS -->
